{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "^1.5.19",
    "@umijs/test": "^3.2.11",
    "axios": "^0.20.0",
    "css-vars-ponyfill": "^2.3.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux-logger": "^3.0.6",
    "umi": "^3.2.14",
    "vditor": "^3.4.5",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-ui": "^2.2.2"
  }
}
